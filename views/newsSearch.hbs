
<head>
    <title>News</title>
    <link rel="stylesheet" href="/formatting.css">
</head>
<body>
    {{#each data}}
    <section>
        <h1>{{title}}</h1>
        <h3>{{date}}</h3>
        <img src="{{imageSrc}}" alt="{{imageAlt}}">
        <p>{{{content}}}</p>
    </section>
    {{/each}}
    <p><a href="/news">Back to News</a></p>
    <script src="/localnews.js"></script>
</body>